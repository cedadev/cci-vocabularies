@prefix dc: <http://purl.org/dc/terms/> .
@prefix gcos: <http://localhost/gcos> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .


#
# concept scheme
#

gcos:ecvConceptScheme a skos:ConceptScheme ;
    skos:prefLabel "A SKOS Concept Scheme for GCOS ECV"@en ;
    skos:definition "Concepts used within GCOS"@en ;
    skos:hasTopConcept gcos:atmospheric ;
    skos:hasTopConcept gcos:terrestrial ;
    skos:hasTopConcept gcos:oceanic ;
    dc:creator [ foaf:name "Antony Wilson" ] ;
    dc:date "2015-09-02 16:29:24" .

#
# gcos concepts
#

gcos:atmospheric a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrial a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "terrestrial"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanic a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericSurface a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric surface"@en ;
    skos:transitiveBroader gcos:atmospheric ;
    skos:definition "Including measurements at standardized, but globally varying heights in close proximity to the surface"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericUpperair a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric upper-air"@en ;
    skos:transitiveBroader gcos:atmospheric ;
    skos:definition "Up to the stratopause"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericComposition a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric composition"@en ;
    skos:transitiveBroader gcos:atmospheric ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSurface a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic surface"@en ;
    skos:transitiveBroader gcos:oceanic ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSubsurface a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic sub-surface"@en ;
    skos:transitiveBroader gcos:oceanic ;
    skos:definition "Including measurements within the surface mixed layer, usually within the upper 15m"@en ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericUpperairCloudProperties a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric upper-air cloud properties"@en ;
    skos:transitiveBroader gcos:atmosphericUpperair ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericCompositionOzoneAndAerosols a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric composition ozone and aerosols"@en ;
    skos:transitiveBroader gcos:atmosphericComposition ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericCompositionCarbonDioxideMethane a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric composition carbon dioxide, methane"@en ;
    skos:transitiveBroader gcos:atmosphericComposition ;
    dc:date "2015-09-02 16:29:24" .

gcos:atmosphericCompositionOzoneAndAerosols a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "atmospheric composition ozone and aerosols"@en ;
    skos:transitiveBroader gcos:atmosphericComposition ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSurfaceColour a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic surface colour"@en ;
    skos:transitiveBroader gcos:oceanicSurface ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSurfaceSeaLevel a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic surface sea level"@en ;
    skos:transitiveBroader gcos:oceanicSurface ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSurfaceSeasurfaceTemperature a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic surface seasurface temperature"@en ;
    skos:transitiveBroader gcos:oceanicSurface ;
    dc:date "2015-09-02 16:29:24" .

gcos:oceanicSurfaceSeaIce a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "oceanic surface sea ice"@en ;
    skos:transitiveBroader gcos:oceanicSurface ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrialFireDisturbance a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "terrestrial fire disturbance"@en ;
    skos:transitiveBroader gcos:terrestrial ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrialGlaciersAndIceCaps a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "terrestrial glaciers and ice caps"@en ;
    skos:transitiveBroader gcos:terrestrial ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrialLandCover a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "terrestria land cover"@en ;
    skos:transitiveBroader gcos:terrestrial ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrialSoilMoisture a skos:Concept ;
    skos:inScheme gcos:ecvConceptScheme ;
    skos:prefLabel "terrestrial soil moisture"@en ;
    skos:transitiveBroader gcos:terrestrial ;
    dc:date "2015-09-02 16:29:24" .

gcos:terrestrialIceSheets a skos:Concept ;
