#
# The Concept scheme for GRIB, taken from the values provided by Martin Juckes
#

@prefix dc: <http://purl.org/dc/terms/> .
@prefix grib: <http://localhost/grib> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix skos: <http://www.w3.org/2008/05/skos#> .


#
# Concept Scheme
#

grib:gribConceptScheme a skos:ConceptScheme ;
	skos:prefLabel "A SKOS Concept Scheme for GRIB"@en ;
	skos:definition "Concepts used within GRIB"@en ;
	dc:creator [ foaf:name "Antony Wilson" ] ;
	dc:date "2015-08-20 08:30:00.0" .


#
# concepts
#

grib:meteorology a skos:Concept ;
	skos:inScheme grib:gribConceptScheme ;
	skos:prefLabel "meteorology"@en ;
	skos:definition ""@en ;
	dc:date "2015-08-20 08:30:00.0" .

grib:oceanography a skos:Concept ;
	skos:inScheme grib:gribConceptScheme ;
	skos:prefLabel "oceanography"@en ;
	skos:definition ""@en ;
	dc:date "2015-08-20 08:30:00.0" .

grib:hydrology a skos:Concept ;
	skos:inScheme grib:gribConceptScheme ;
	skos:prefLabel "hydrology"@en ;
	skos:definition ""@en ;
	dc:date "2015-08-20 08:30:00.0" .

grib:landSurface a skos:Concept ;
	skos:inScheme grib:gribConceptScheme ;
	skos:prefLabel "land surface"@en ;
	skos:definition ""@en ;
	dc:date "2015-08-20 08:30:00.0" .
