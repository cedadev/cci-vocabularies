cci_freq_concept:freq_sec a skos:Concept, cci_ontology:freq;
    skos:inScheme cci_freq_scheme: ;
    skos:prefLabel "second"@en ;
    skos:altLabel "P0DT0H0M1S"@en ;
    skos:topConceptOf cci_freq_scheme: ;
.

cci_freq_coll: skos:member cci_freq_concept:freq_sec .

cci_freq_scheme: skos:hasTopConcept cci_freq_concept:freq_sec .

